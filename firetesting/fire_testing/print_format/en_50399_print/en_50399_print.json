\\n  <tr>\\n  <tr>\\n    <td>Laboratory</td>\\n    <td></td>\\n  </tr>\\n  <tr>\\n    <td>Certificate date</td>\\n    <td>{{ doc.certificate_date }}</td>\\n  </tr>\\n</table>\\n\\n<p><br></p>\\n\\n<center><h3>Conclusions</h3></center>\\n<p>Burner output: {{ doc.burner_output }} kW</p>\\n<table style=\\\"width: 100%;\\\">\\n  <tr>\\n    <td class=\\\"blackborder\\\" colspan=4><center>Average of 1 test</center></td>\\n  </tr>\\n  <tr>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">Peak HRR<sub>avg</sub> [kW]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.peak_hrr }}</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">Peak SPR<sub>avg</sub> [m<sup>2</sup>/s]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.peak_spr }}</td>\\n  </tr> \\n  <tr>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">THR<sub>1200s</sub> [MJ]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.thr_1200s }}</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">TSP<sub>1200s</sub> [m<sup>2</sup>]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.tsp_1200s }}</td>\\n  </tr> \\n  <tr>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">FIGRA<sub>0.4MJ</sub> [W/s]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.figra }}</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-right: none;\\\">Flame Spread [m]</td>\\n    <td class=\\\"blackborder\\\" style=\\\"border-left: none;\\\">{{ doc.flame_spread }}</td>\\n  </tr> \\n</table>\\n\\n<div class=\\\"page-break\\\"></div>\\n<!-- ------ PAGE 2 ----- -->\\n\\n<center><h3>Sample description</h3></center>\\n<table style=\\\"width: 100%;\\\">\\n  <tr>\\n    <td class=\\\"blackborder\\\" colspan=2><center>Material data</center></td>\\n  </tr>\\n  <tr>\\n    <td class=\\\"blackborder\\\" style=\\\